{
  "compilerOptions": {
    "target": "ES5",
    "module": "system",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "tsbuild"
  },
  "filesGlob": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "files": [
    "./src/component/about/about.ts",
    "./src/component/app/app.ts",
    "./src/component/chapters.ts",
    "./src/component/codemirror.ts",
    "./src/component/course/course.ts",
    "./src/component/exercises.ts",
    "./src/component/hello.ts",
    "./src/component/home/home.ts",
    "./src/component/navigation/navigation.ts",
    "./src/component/panel.ts",
    "./src/main.ts",
    "./src/service/httpservice.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  }
}
